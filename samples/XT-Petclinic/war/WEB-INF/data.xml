    </petclinic:vet>
    <petclinic:vet id="5" firstName="Henry" lastName="Stevens">
      <petclinic:specialty id="1"/>
    </petclinic:vet>
    <petclinic:vet id="6" firstName="Sharon" lastName="Jenkins"/>
  </util:set>

  <!-- visits -->
  <petclinic:visit id="1" date="1996-03-04" description="rabies shot"/>
