<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<!--
  - Application context definition for PetClinic using In-Memory Storage.
  -->
<beans>

	<!-- ========================= BUSINESS OBJECT DEFINITIONS ========================= -->

  <bean id="clinic" class="org.springframework.samples.petclinic.dao.InMemoryClinic">
    <property name="petTypes" ref="petTypes"/>
    <property name="vets" ref="vets"/>
    <property name="owners" ref="owners"/>
  </bean>

</beans>
