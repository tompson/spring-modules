CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE BOOK(BOOK_ID INTEGER NOT NULL,BOOK_NAME VARCHAR(255) NOT NULL,CONSTRAINT PK_BOOK PRIMARY KEY(BOOK_ID))
CREATE MEMORY TABLE BOOK_PAGE(BOOK_PAGE_ID INTEGER NOT NULL,BOOK_ID INTEGER NOT NULL,BOOK_PAGE_TEXT VARCHAR,CONSTRAINT PK_BOOK_PAGE PRIMARY KEY(BOOK_PAGE_ID,BOOK_ID))
CREATE MEMORY TABLE CATEGORY(CATEGORY_ID INTEGER NOT NULL,CATEGORY_NAME VARCHAR,CONSTRAINT PK_CATEGORY PRIMARY KEY(CATEGORY_ID))
CREATE MEMORY TABLE DOCUMENT_ID(NEXT_DOCUMENT_ID INTEGER NOT NULL,CONSTRAINT PK_DOCUMENT_ID PRIMARY KEY(NEXT_DOCUMENT_ID))
CREATE USER SA PASSWORD ""
GRANT DBA TO SA
SET WRITE_DELAY 60
SET SCHEMA PUBLIC
