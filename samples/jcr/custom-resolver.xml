<ivyconf>
	<resolvers>
		<chain name="custom">
			<!-- day software public maven repository -->
			<ibiblio name="day" root="http://www.day.com/maven"/>
			<!-- apache incubator maven / made an url to respect the naming
			<ibiblio name="apache-incubator" root="http://cvs.apache.org/dist/incubator/"/>
			-->
			<!---->
			<url name="apache-incubator">			
				<artifact pattern="http://cvs.apache.org/repository/org.apache.jackrabbit/jars/[artifact]-[revision]-incubating.[ext]" />				
			</url>
			
			<!-- aparzev/jeceira -->
			<url name="jeceira">
				<!-- url pattern for jeceira wiki -->
		        <artifact pattern="http://www.jeceira.com/confluence/download/attachments/3200/[artifact]-[revision].[ext]?version=1" />
			</url>
			<!-- jug -->
			<url name="jug">
				<!-- url pattern for jug -->
		        <artifact pattern="http://jug.safehaus.org/curr/[artifact]-[revision].[ext]" />
			</url>
		</chain>
	</resolvers>
</ivyconf>
