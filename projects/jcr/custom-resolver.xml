<ivyconf>
	<resolvers>
		<chain name="custom">
			<!-- day software public maven repository -->
			<ibiblio name="day" root="http://www.day.com/maven/jsr170" pattern="jars/[artifact]-[revision].[ext]"/>
			<!-- apache incubator maven / made an url to respect the naming
			<ibiblio name="apache-incubator" root="http://cvs.apache.org/dist/incubator/"/>
			-->
			<!--
			<url name="jackrabbit-maven">			
				<artifact pattern="http://ibiblio.org/maven/org.apache.jackrabbit/jars/[artifact]-[revision].[ext]" />				
			</url>
			-->
			<!-- 
			<ibiblio name="jackrabbit-maven" m2compatible="true" root="http://ibiblio.org/maven/org/"/>
			-->
			<!-- aparzev/jeceira -->
			<url name="jeceira">
				<!-- url pattern for jeceira wiki -->
		        <artifact pattern="http://www.jeceira.com/confluence/download/attachments/3200/[artifact]-[revision].[ext]?version=1" />
			</url>
			<!-- jug -->
			<url name="jug">
				<!-- url pattern for jug -->
		        <artifact pattern="http://jug.safehaus.org/curr/[artifact]-[revision].[ext]" />
			</url>
		</chain>
	</resolvers>
</ivyconf>
