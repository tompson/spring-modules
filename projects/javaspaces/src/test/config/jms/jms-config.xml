<?xml version="1.0"?>
<com>
  <j_spaces>
	<jms>
		<!-- GigaSpaces JMS JNDI registry bonded names which the client performs lookup() with. -->
		<jndi-bonded-names>
			<!-- GigaSpaces jndi rmiReg bonded names  -->
			<connections>
                <!-- the container name which is associated to this jms connection factory. 
                	Default is myJMS_container -->
                <container-prefix>${com.gs.jms.container-prefix}</container-prefix>
                <!-- the space name which is associated to this jms connection factory. 
                	Default is myJMS -->
                <space-prefix>${com.gs.jms.space-prefix}</space-prefix>
				<!-- The space url which is used in the bonded JMS ConnectionFactory. 
					Default is /./myJMS?schema=cache -->
				<overwrite-space-url>${com.gs.jms.url}</overwrite-space-url>
			</connections>
		</jndi-bonded-names>
		<!-- Enabling JMS client debug options 
		Default is false -->
		<jms-debug>${com.gs.jms.debug}</jms-debug>
    	<jms-debug-date>false</jms-debug-date>
	</jms>
  </j_spaces>
</com>
